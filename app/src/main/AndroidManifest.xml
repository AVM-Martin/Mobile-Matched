<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.my.avmmartin.matched">

    <application
        android:name=".Matched"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.account.AccountActivity"></activity>
        <activity android:name=".ui.schedule.free.FreeScheduleActivity" />
        <activity android:name=".ui.approve.list.ApprovalActivity" />
        <activity android:name=".ui.schedule.add.AddActivity" />
        <activity android:name=".ui.schedule.view.Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
